{
  "name": "momentjs-vs-date-fns-performance-test",
  "version": "1.0.0",
  "description": "Performance tests for date-fns and momentjs",
  "scripts": {
    "compile": "rimraf build/ && babel src/ --out-dir build",
    "test": "npm run --silent compile && npm run --silent bench:get-test && npm run --silent bench:manipulate-test && npm run --silent bench:query-test",
    "bench:get-test": "node lib/get-test",
    "bench:parse-test": "node lib/parse-test",
    "bench:manipulate-test": "node lib/manipulate-test",
    "bench:display-test": "node lib/display-test",
    "bench:query-test": "node lib/query-test",
    "bundle": "babel-node src/size-test",
    "lint": "eslint src/"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.7",
    "beautify-benchmark": "^0.2.4",
    "benchmark": "^2.1.2",
    "cli-table": "^0.3.1",
    "date-fns": "^2.14.0",
    "easy-table": "^1.0.0",
    "moment": "^2.27.0",
    "rimraf": "~2.5.4",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-preset-node6": "^11.0.0",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.7.1"
  }
}
